      </div>\n\n        <div className=\"m-1 w-100\">\n          <button className=\"btn\" onClick={addtocart}>ADD TO CART</button>\n        </div>\n      </div>\n\n      <Modal show={show} onHide={handleClose}>\n        <Modal.Header closeButton>\n          <Modal.Title>{clothe.name}</Modal.Title>\n        </Modal.Header>\n\n        <Modal.Body>\n          <img\n            src={clothe.image}\n            className=\"img-fluid\"\n            style={{ height: \"400px\" }}\n            alt=\"clothe\"\n          />\n          <p>{clothe.description}</p>\n        </Modal.Body>\n\n        <Modal.Footer>\n          <button className=\"btn\" onClick={addtocart}>ADD TO CART</button>\n          <button className=\"btn\" onClick={handleClose}>\n            close\n          </button>\n        </Modal.Footer>\n      </Modal>\n    </div>\n  );\n}\n","/home/elithecompguru/Documents/RapidHyena@2020/Programmer's_Bay/WEB_Programming/projects/MERN-STACK/mine/mern-pizza/client/src/store.js",[],"/home/elithecompguru/Documents/RapidHyena@2020/Programmer's_Bay/WEB_Programming/projects/MERN-STACK/mine/mern-pizza/client/src/reducers/clothesReducers.js",[],"/home/elithecompguru/Documents/RapidHyena@2020/Programmer's_Bay/WEB_Programming/projects/MERN-STACK/mine/mern-pizza/client/src/actions/clotheActions.js",[],"/home/elithecompguru/Documents/RapidHyena@2020/Programmer's_Bay/WEB_Programming/projects/MERN-STACK/mine/mern-pizza/client/src/clothesdata.js",[],["67","68"],"/home/elithecompguru/Documents/RapidHyena@2020/Programmer's_Bay/WEB_Programming/projects/MERN-STACK/mine/mern-pizza/client/src/reducers/cartReducer.js",[],"/home/elithecompguru/Documents/RapidHyena@2020/Programmer's_Bay/WEB_Programming/projects/MERN-STACK/mine/mern-pizza/client/src/actions/cartActions.js",[],{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":1,"column":8,"nodeType":"75","messageId":"76","endLine":1,"endColumn":17},{"ruleId":"73","severity":1,"m